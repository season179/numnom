{
  "manifest_version": 3,
  "name": "NumNom",
  "version": "1.2.3",
  "description": "Download stock price and dividend data",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "NumNom",
    "default_popup": "popup.html"
  }
}
